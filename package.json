{
  "name": "vibelink-0372",
  "version": "1.0.0",
  "description": "VibeLink 0372® - Complete Social Media PWA Platform - Where the World Vibe Starts",
  "main": "index.html",
  "type": "module",
  "scripts": {
    "dev": "live-server --port=3000 --host=localhost --entry-file=index.html --wait=100 --ignore=node_modules,.git,*.md",
    "build": "npm run build:css && npm run build:js && npm run build:assets && npm run build:sw && npm run build:manifest && npm run optimize",
    "build:css": "postcss style.css -o dist/style.min.css --map",
    "build:js": "rollup -c rollup.config.js",
    "build:assets": "node scripts/copy-assets.js",
    "build:sw": "workbox injectManifest workbox-config.js",
    "build:manifest": "node scripts/generate-manifest.js",
    "optimize": "node scripts/optimize.js",
    "preview": "serve dist -p 3000",
    "deploy": "npm run build && npm run deploy:gh-pages",
    "deploy:gh-pages": "gh-pages -d dist",
    "deploy:production": "npm run build && node scripts/deploy-production.js",
    "test": "jest --config jest.config.js",
    "test:unit": "jest --config jest.config.js --testPathPattern=__tests__/unit",
    "test:integration": "jest --config jest.config.js --testPathPattern=__tests__/integration",
    "test:security": "node scripts/security-test.js",
    "test:performance": "node scripts/performance-test.js",
    "test:lighthouse": "lighthouse http://localhost:3000 --output html --output-path ./reports/lighthouse.html",
    "lint": "eslint . --ext .js --fix",
    "lint:css": "stylelint '**/*.css' --fix",
    "lint:html": "html-validate *.html",
    "security:audit": "npm audit --audit-level high",
    "security:scan": "node scripts/security-scan.js",
    "security:encryption-test": "node scripts/test-encryption.js",
    "performance:budget": "node scripts/performance-budget.js",
    "performance:analyze": "webpack-bundle-analyzer dist/stats.json",
    "pwa:audit": "pwa-asset-generator assets/logo.svg assets -i index.html -m manifest.json --dark-mode --opaque false --padding 0",
    "pwa:validate": "pwabuilder --url https://thabang0372.github.io/VibeLink-0372/",
    "docs:generate": "jsdoc -c jsdoc.json",
    "docs:serve": "serve docs -p 4000",
    "clean": "rimraf dist docs coverage reports",
    "clean:all": "npm run clean && rimraf node_modules package-lock.json",
    "postinstall": "npm run build && npm run security:audit",
    "predeploy": "npm run test && npm run security:scan",
    "start": "npm run dev",
    "serve": "npm run preview"
  },
  "keywords": [
    "pwa",
    "social-media",
    "progressive-web-app",
    "real-time",
