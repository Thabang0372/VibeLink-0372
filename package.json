{
  "name": "vibelink-0372",
  "version": "1.0.0",
  "description": "VibeLink 0372Â® - Complete Social Platform PWA - Where the World Vibe Starts",
  "type": "module",
  "main": "script.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run watch\"",
    "server": "node server.js",
    "build": "npm run build:css && npm run build:js && npm run build:html && npm run build:assets && npm run build:sw",
    "build:css": "postcss style.css -o dist/style.min.css --map --env production",
    "build:js": "rollup -c rollup.config.js",
    "build:html": "node build-html.js",
    "build:assets": "node build-assets.js",
    "build:sw": "workbox injectManifest workbox-config.js",
    "watch": "chokidar '**/*.css' '**/*.js' '**/*.html' -c 'npm run build'",
    "test": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:unit": "jest --config jest.config.js",
    "test:integration": "node tests/integration.js",
    "test:e2e": "playwright test",
    "lint": "eslint . --ext .js,.html --fix",
    "lint:css": "stylelint '**/*.css' --fix",
    "audit": "npm audit --audit-level=critical && npm run audit:ci",
    "audit:ci": "audit-ci --config ./audit-ci.json",
    "security": "npm run security:scan && npm run security:audit",
    "security:scan": "snyk test && snyk monitor",
    "security:audit": "node security-audit.js",
    "docker:build": "docker build -t vibelink-0372 .",
    "docker:run": "docker run -d -p 80:80 -p 443:443 --name vibelink-app vibelink-0372",
    "docker:compose": "docker-compose up -d",
    "docker:production": "docker-compose -f docker-compose.prod.yml up -d",
    "deploy": "npm run build && npm run deploy:gh-pages",
    "deploy:gh-pages": "gh-pages -d dist",
    "deploy:netlify": "netlify deploy --prod --dir=dist",
    "deploy:vercel": "vercel --prod",
    "deploy:aws": "node deploy-aws.js",
    "analyze": "npm run analyze:bundle && npm run analyze:performance",
    "analyze:bundle": "webpack-bundle-analyzer dist/stats.json",
    "analyze:performance": "lighthouse https://thabang0372.github.io/VibeLink-0372/ --output json --output html --output-path ./lighthouse",
    "monitor": "npm run monitor:performance && npm run monitor:security",
    "monitor:performance": "node monitor-performance.js",
    "monitor:security": "node monitor-security.js",
    "backup": "node backup-database.js",
    "restore": "node restore-database.js",
    "migrate": "node database-migrations.js",
    "seed": "node database-seeder.js",
    "clean": "rimraf dist build coverage .cache",
    "preinstall": "node preinstall-check.js",
    "postinstall": "node postinstall-setup.js",
    "prebuild": "npm run clean && npm run lint && npm run test",
    "postbuild": "npm run analyze && npm run security",
    "prerelease": "npm run build && npm run test && npm run audit",
    "release": "standard-version",
    "postrelease": "git push --follow-tags origin main && npm run deploy"
  },
  "keywords": [
    "pwa",
    "social-media",
    "real-time",
    "encryption",
    "marketplace",
    "streaming",
    "wallet",
    "ai",
    "analytics",
    "offline",
    "progressive-web-app",
    "vibelink",
    "enterprise",
    "secure",
    "parse-server",
    "mongodb",
    "redis",
    "docker",
    "nginx",
    "websockets"
  ],
  "author": {
    "name": "VibeLink 0372 Team",
